+static MDFN_COLD void Cleanup(void)
+{
+ #ifdef WANT_DEBUGGER
+ PCEDBG_Kill();
+ #endif
+
+ if(PCE_IsCD)
+ {
+  PCECD_Close();
+ }
+
+ if(IsHES)
+  HES_Close();
+ else
+ {
+  HuC_Kill();
+ }
+
+ if(vce)
+ {
+  delete vce;
+  vce = NULL;
+ }
+
+ if(psg)
+ {
+  delete psg;
+  psg = NULL;
+ }
+
+ for(unsigned ch = 0; ch < 2; ch++)
+ {
+  if(HRBufs[ch])
+  {
+   delete HRBufs[ch];
+   HRBufs[ch] = NULL;
+  }
+
+  if(CDDABufs[ch])
+  {
+   delete CDDABufs[ch];
+   CDDABufs[ch] = NULL;
+  }
+ }
+
+ if(ADPCMBuf)
+ {
+  delete ADPCMBuf;
+  ADPCMBuf = NULL;
+ }
+
+ if(HRRes)
+ {
+  delete HRRes;
+  HRRes = NULL;
+ }
+}

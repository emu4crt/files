+template<typename T>
+static INLINE void Doubleize(T* ptr, const int orig_len)
+{
+ for(int i = orig_len - 1; MDFN_LIKELY(i >= 0); i--)
+ {
+  auto tmp = *(ptr + i);
+
+  *(ptr + (i << 1) + 0) = tmp;
+  *(ptr + (i << 1) + 1) = tmp;
+ }
+}

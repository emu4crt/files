+ PCE_ACEnabled = MDFN_GetSettingB("pce.arcadecard");
+
+ HuCPU.Init(IsHES);
+
+ for(int x = 0; x < 0x100; x++)
+ {
+  HuCPU.SetFastRead(x, NULL);
+  HuCPU.SetReadHandler(x, PCEBusRead);

+static MDFN_COLD uint8 CheatMemRead(uint32 A)
+{
+ const uint8* p = CheatMemGetPointer(A);
+
+ if(p)
+  return *p;
+
+ return 0x00;
+}
+
+static MDFN_COLD void CheatMemWrite(uint32 A, uint8 V)
+{
+ uint8* p = CheatMemGetPointer(A);
+
+ if(p)
+  *p = V;
+}

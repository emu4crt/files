+        if(qtrecfn)	// Needs to be before MDFNI_Closegame() for now
+         MDFNI_StopAVRecord();
+
+        if(soundrecfn)
+         MDFNI_StopWAVRecord();
+
+	if(MDFN_GetSettingB("autosave") && !autosave_load_error)
+	 MDFNI_SaveState(NULL, "mca", NULL, NULL, NULL);
+
+	MDFNI_NetplayDisconnect();
+
+	Debugger_Kill();
+
+	MDFNI_CloseGame();
+
+	RMDUI_Kill();
+	Input_GameClosed();
+	Sound_Kill();
+
+	CurGame = NULL;
+
+	return(1);

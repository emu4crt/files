+static MDFN_COLD bool TestMagic(GameFile* gf)
+{
+ if(gf->ext == "sfc" || gf->ext == "smc" || gf->ext == "swc" || gf->ext == "fig")
+  return true;
+
+ if(SNSFLoader::TestMagic(gf->stream))
+  return true;
+
+ if(SPCReader::TestMagic(gf->stream))
+  return true;
+
+ return false;
+}

+static void RBGPP(const unsigned layer, uint64* buf, const unsigned rbg_w)
+{
+ ApplyHMosaic(layer, buf, rbg_w);
+
+ for(unsigned i = 0; i < rbg_w; i++)
+ {
+  uint64 tmp = buf[i];
+
+  if(LB.rotabsel[i])
+   tmp &= ~(uint64)0xFFFFFFFF;
+
+  buf[i] = tmp;
+ }
+
+ if(HRes & 0x2)
+  Doubleize(buf, rbg_w);
+
+ ApplyWin(layer, buf);
+}

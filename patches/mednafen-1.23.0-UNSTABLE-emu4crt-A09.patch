+  }
+  else
+  { // NTSC
+    if ((V & 32) == 32){resolution_to_change_h = 480;} //5
+    else {resolution_to_change_h = 240;}
+  }
+  
+  if(prev_resolution_w != resolution_to_change_w || prev_resolution_h != resolution_to_change_h)

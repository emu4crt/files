+static INLINE uint32 rgb15_to_rgb24(uint16 src)
+{
+ return ((((src << 3) & 0xF8) | ((src << 6) & 0xF800) | ((src << 9) & 0xF80000) | ((src << 16) & 0x80000000)));;
+}
